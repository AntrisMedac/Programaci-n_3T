package Map;

public class Producto {

	String nombre;
	int stock;
	double precio;
	
	public Producto(String nombre, int stock, double precio) {
		this.nombre = nombre;
		this.stock = stock;
		this.precio = precio;
	}

	@Override
	public String toString() {
		return "Producto " + nombre + ", stock=" + stock + ", precio=" + precio;
	}
	
}
